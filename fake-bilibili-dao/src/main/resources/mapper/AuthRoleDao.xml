<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.fake.bilibili.dao.AuthRoleDao">
    <select id="getRoleByCode" resultType="com.fake.bilibili.domain.auth.AuthRole" parameterType="java.lang.String">
        select
            *
        from
            t_auth_role
        where
            code = #{code}
    </select>
</mapper>