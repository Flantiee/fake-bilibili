<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.fake.bilibili.dao.UserMomentsDao">

    <insert id="addUserMoments" parameterType="com.fake.bilibili.domain.UserMoment" useGeneratedKeys="true" keyProperty="id">
        insert into t_user_moments(
                userId,
                `type`,
                contentId,
                createTime,
        )values(
                #{userId},
                #{`type`},
                #{contentId},
                #{createTime},
        )

    </insert>
</mapper>